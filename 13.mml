    <mo>)</mo>
  </mrow>

  <msup>
    <mrow>
      <mo minsize="150%">|</mo>
      <mi>&#207;&#134; <!-- \varphi --></mi>
      <mo stretchy="false">(</mo>
      <mi>x</mi>
      <mo>+</mo>

      <mi mathvariant="normal">i</mi>
      <mi>y</mi>
      <mo stretchy="false">)</mo>
      <mo minsize="150%">|</mo>
    </mrow>
    <mn>2</mn>

  </msup>
  <mo>=</mo>
  <mn>0</mn>
</mrow>
